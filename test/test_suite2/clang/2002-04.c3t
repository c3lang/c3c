fn int main2() {
  while (foo()) {
     switch (foo()) {
     case 0:
     case 1:
     case 2:
     case 3:
       printf("3");
       nextcase;
     case 4: printf("4"); nextcase;
     case 5:
     case 6:
     default:
     }
   }
   return 0;
}

