module test;

extern fn void printf(char *string, ...);

macro checker(int x, $i)
{
    var $indent = 3 - $i;
    for (int i = 0; i < $indent; i++) printf(" ");
    printf("Enter %d\n", $i);
    while FOO: (x-- > 0)
    {
        for (int i = 0; i < $indent; i++) printf(" ");
        printf("Helo %d\n", x);
        $if ($i > 0):
            checker(x, $i - 1);
        $endif;
        if (x % 2 == 0) break FOO;
    }
    for (int i = 0; i < $indent; i++) printf(" ");
    printf("Exit %d\n", $i);
}

fn void main()
{
    int ab = 7;
    checker(ab, 3);
}

