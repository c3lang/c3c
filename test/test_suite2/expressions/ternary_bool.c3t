{

    bool b = true;
    bool* c = &b;
    if (*c ? *c : *c) printf("Woa!\n");
    if (b ? b : b) printf("Woa!\n");
    if (*c ?: *c) printf("Woa!\n");
    if (b ?: b) printf("Woa!\n");
}

