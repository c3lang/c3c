// #target: macos-x64
module test;
enum TokenType: char (char token) {
  ERROR = 0,
  INCP = '>',
  DECP = '>',
  LOOPS = '[',
}

macro populate_token_table() {
  TokenType[256] $tmp = {};

  $foreach ($tokenType: TokenType.values)
    $tmp[$tokenType.token] = $tokenType;
  $endforeach

  return $tmp;
}

const TokenType[256] TOKEN_TABLE = populate_token_table();

