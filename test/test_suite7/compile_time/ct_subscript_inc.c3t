// #target: macos-x64
module test;
import std;

fn void main()
{
	String $c = "Abcd";
	io::printn($c);
	io::printfn("%c", $c[1]++);
	io::printfn("%c", $c[2]++);
	io::printfn("%c", $c[3]--);
	io::printn($c);

	String $d = "Abcd";
	io::printn($d);
	io::printfn("%c", ++$d[1]);
	io::printfn("%c", ++$d[2]);
	io::printfn("%c", --$d[3]);
	io::printn($d);

	var $e = { 1, 2, 3 };
	io::printn((int[?])$e);
   	io::printn($e[1]++);
	io::printn((int[?])$e);

	int[?] $f = { 1, 2, 3 };
   	io::printn($f);
   	io::printn($f[1]++);
   	io::printn($f);

	int[?] $g = { 1, 2, 3 };
   	io::printn($g);
   	io::printn(--$g[1]);
   	io::printn(--$g[0]);
   	io::printn($g);

}

