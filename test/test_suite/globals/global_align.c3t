// #target: macos-x64

module abc;

int foo @align(32);

fn void test()
{
	int x = foo;
}

// #expect: abc.ll

@abc.foo = local_unnamed_addr global i32 0, align 32

define void @abc.test() #0 {
entry:
  %x = alloca i32, align 4
  %0 = load i32, i32* @abc.foo, align 32
  store i32 %0, i32* %x, align 4
  ret void
}
