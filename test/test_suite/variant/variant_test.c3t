// #target: x64-darwin
module foo;
extern fn void printf(char*, ...);

fn void test(variant x)
{
    switch (x.typeid)
    {
        case int:
            printf("Was int\n");
        case double:
            printf("Was double\n");
        case variant:
            printf("Was variant\n");
        case int*:
            printf("Was int*\n");
        default:
            printf("Unknown type\n");
    }
}

fn void test_all(variant... y)
{
    foreach (element : y)
    {
        test(element);
    }
}

fn void main()
{
    variant x = &&1;
    int z;
    variant y = &z;
    typeid g = y.typeid;
    typeid h = x.typeid;
    if (y.typeid == int.typeid)
    {
        printf("y int match\n");
    }
    if (x.typeid == int.typeid)
    {
        printf("x int match\n");
    }
    y = &&1.0;
    x = &x;
    if (y.typeid == int.typeid)
    {
        printf("y int match\n");
    }
    if (x.typeid == int.typeid)
    {
        printf("x int match\n");
    }
    test(x);
    test(&&1.0);
    test(&&1);
    test(&&true);
    printf("----\n");
    int* df = null;
    test_all(x, x, &&1.0, &x, &df);
}

