// #target: macos-x64
module foo;

extern fn void printf(char*, ...);

fn void test(any z)
{
    switch (z)
    {
        case int:
            printf("int: %d\n", *z);
        case double:
            printf("double %f\n", *z);
        default:
            printf("Unknown type.\n");
    }
}
fn void test2(any y)
{
    switch (z = y)
    {
        case int:
            y = &&12;
            printf("int: %d\n", *z);
        case double:
            printf("double %f\n", *z);
        default:
            printf("Unknown type.\n");
    }
}

fn void test3(any y)
{
    switch (z = *y)
    {
        case int:
            printf("int: %d\n", z);
        case double:
            printf("double %f\n", z);
        default:
            printf("Unknown type.\n");
    }
}

fn int main()
{
    test(&&123.0);
    test(&&1);
    test(&&true);
    test2(&&123.5);
    test2(&&1);
    test2(&&true);
    test3(&&124.0);
    test3(&&2);
    test3(&&true);
    return 0;
}

