// #target: macos-aarch64
module gen(<Type>);

fn Type mult(Type x)
{
    return x * x;
}

fn Type addMult(Type x, Type a, Type b)
{
    return x * a + b;
}

module test;
import gen;

def intMult = gen::mult(<int>);
def doubleAddMult = gen::addMult(<double>);

fn int getIt(int i)
{
    return intMult(i) + 1;
}

fn double getIt2(double i)
{
    return doubleAddMult(i, 2, 3);
}

