module foo;
struct Foo
{
    int[] x;
}

struct FooIterator
{
    usize index;
    Foo *f;
}
macro FooIterator Foo.iterator(Foo *f)
{
    return FooIterator { 0, f };
}

macro bool FooIterator.next(FooIterator *it, int *value)
{
    if (it.index == it.f.x.len()) return false;
    *value = it.f.x[it.index++];
    return true;
}

func void main()
{
    int[*] i = { 1, 3, 10 };
    Foo x = { &i };
    foreach FOO: (int f : x) {
        printf("%d\n", f);
        while (1)
        {
            break FOO;
        }
    }
}

extern func int printf(char *fmt, ...);

