// #target: macos-x64
module foo;
struct Foo
{
    int[] x;
}

macro int Foo.@operator_element_at(Foo* &foo, usz index) @operator([])
{
    return foo.x[index];
}

macro usz Foo.@operator_len(Foo* &foo) @operator(len)
{
    return foo.x.len;
}

fn void main()
{
    int[*] i = { 1, 3, 10 };
    Foo x = { &i };
    foreach FOO: (int f : x) {
        printf("%d\n", f);
        while (1)
        {
            break FOO;
        }
    }
}

extern fn int printf(char *fmt, ...);

