// #target: x64-darwin

module foo;
struct Foo
{
    int[] x;
}

macro int Foo.operator_element_at(Foo &foo, usize index)
{
    return foo.x[index];
}

macro usize Foo.operator_len(Foo &foo)
{
    return foo.x.len;
}

fn void main()
{
    int[*] i = { 1, 3, 10 };
    Foo x = { &i };
    foreach FOO: (int f : x) {
        printf("%d\n", f);
        while (1)
        {
            break FOO;
        }
    }
}

