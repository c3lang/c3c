// #target: macos-x64
module mymodule;

extern fn void printf(char *, ...);

faultdef FOO;
faultdef BAR, BAZ;

fn void test()
{
    int? x = BAR~;

    typeid z = int.typeid;
    switch (z)
    {
        case int:
            printf("Was int!\n");
        case bool:
            nextcase double;
        case double:
            nextcase;
        default:
            printf("Wasn't int :(\n");
    }
    int a = 1;
    int b = 2;
    int zy = b + a;
    switch (zy)
    {
        case a:
            printf("Got a\n");
        case b:
            printf("Got b\n");
        default:
            printf("None of the above\n");
    }
    switch
    {
        case a < 0:
            printf("A negative\n");
        case a == 1:
            printf("A is 1\n");
        default:
            printf("Unexpected a\n");
    }
}
fn void main()
{
    test();
    printf("Hello!\n");
}

