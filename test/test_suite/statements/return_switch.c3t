switch.entry:
  %1 = load i32, i32* %switch
  switch i32 %1, label %switch.default [
    i32 0, label %switch.case
    i32 3, label %switch.case
    i32 1, label %switch.case
    i32 2, label %switch.case1
  ]

switch.case:
  ret i32 2

switch.case1:
  ret i32 3

switch.default:
  ret i32 4
}