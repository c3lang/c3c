// #target: x64-darwin

module foo;
fn int test()
{
    static int x = 0;
    return 1 + x++;
}

extern fn void printf(char*, ...);

fn void main()
{
    int i = 10;
    do
    {
        printf("%d\n", test());
    } while (false);
    do
    {
        printf("%d, %d\n", i, test());
    };
    do
    {
        if (i < 0) continue;
        printf("A\n", i, test());
    };
    do
    {
        if (i < 0) break;
        printf("B\n", i, test());
    };

}

