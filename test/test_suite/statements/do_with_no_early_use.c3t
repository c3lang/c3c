// #target: macos-aarch64
module test;
import std;
fn int main()
{
	int i = 10;
	do
	{
		io::printfn("%s", i);
	} while (@my_ok(nonzero(--i)));
	return 0;
}

macro @my_ok(#z)
{
	if (catch #z) return false;
	return true;
}

faultdef WAS_ZERO;

fn int? nonzero(int x)
{
	if (x == 0) return WAS_ZERO~;
	return x;
}

/* #expect: test.ll

