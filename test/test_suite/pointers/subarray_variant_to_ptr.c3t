// #target: macos-x64
module foo;

extern fn void printf(char*, ...);

fn void test1(any* z)
{
    int* w = z.ptr;
    printf("%d\n", *w);
}

fn void test2(int[] z)
{
    int* w = z.ptr;
    printf("%d\n", *w);
}

fn void main()
{
    int x = 123;
    int y = 293483;
    int[2] w = { 144, 772 };
    test1(&x);
    test2(w[..]);
}

