// #target: x86_64-darwin
module splat;

extern func int sum_us(int... x);

func void test()
{
    sum_us(1, 2, 3);
    int[3] x = { 1, 2, 3 };
    int[] z = &x;
    sum_us(...x);
    sum_us(...z);
    sum_us();
}

