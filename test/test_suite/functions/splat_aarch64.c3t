  %27 = getelementptr inbounds %"int[]", %"int[]"* %vararg5, i32 0, i32 1
  store i64 0, i64* %27, align 8
  %28 = bitcast [2 x i64]* %tempcoerce6 to i8*
  %29 = bitcast %"int[]"* %vararg5 to i8*
