module foo;
import libc;

struct Matrix2x2
{
	union
	{
		struct {
			float m00, m01, m10, m11;
		}
		float[4] m;
	}
}

struct Matrix2x2_b
{
	union
	{
		float[4] m;
		struct {
			float m00, m01, m10, m11;
		}
	}
}

fn void main()
{
	Matrix2x2 m = { 1, 2, 3, 4 };
	Matrix2x2_b m2 = { { 1, 2, 3, 4 } };
	libc::printf("%f %f %f %f\n", m.m00, m.m[1], m.m10, m.m[3]);
}
