// #target: macos-x64
module test;

extern fn void printf(char*, ...);

fn void main()
{

    var $foo = { 1, 10, 34 };

    $foreach $i : $foo:
       printf("Foo %d\n", $i);
    $endforeach;

    $foreach $i, $j : $foo:
        printf("Bar %d %d\n", $i, $j);
    $endforeach;

    $foreach $x : { 123, "abc", 1177, "hello" }:
    {
        $typeof($x) z = $x;
        $switch $typeof($x):
        $case int:
            printf("Bar %d\n", $x);
        $default:
            printf("Bar %s\n", $x);
        $endswitch
    }
    $endforeach;

    $foreach_r $i : $foo:
       printf("Foo %d\n", $i);
    $endforeach;

    $foreach_r $i, $j : $foo:
        printf("Bar %d %d\n", $i, $j);
    $endforeach;

    $foreach_r $x : { 123, "abc", 1177, "hello" }:
    {
        $typeof($x) z = $x;
        $switch $typeof($x):
        $case int:
            printf("Bar %d\n", $x);
        $default:
            printf("Bar %s\n", $x);
        $endswitch
    }
    $endforeach;
}

