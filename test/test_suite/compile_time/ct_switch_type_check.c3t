// #target: x64-darwin

module test;

macro get_type($Type)
{
    $switch ($Type.typeid):
        $case int:
            return "int";
        $case double:
            return "double";
        $default:
            return "any other";
    $endswitch;
}

extern fn void printf(char*, ...);

fn void main()
{
    printf("%s\n", @get_type(int));
    printf("%s\n", @get_type(double));
    printf("%s\n", @get_type(bool));

}

