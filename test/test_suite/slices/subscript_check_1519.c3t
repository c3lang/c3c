// #target: macos-x64
module test;
import std::collections::list;
macro @test_list(a)
{
  var $Type = $typeof(a);
  $if $defined(a[0]) &&& $Type.typeid == List(<$typeof(a[0])>).typeid:
    return 1;
  $else
    return 0;
  $endif
}

fn int main(String[] args)
{
  DString str = dstring::new("test");
  return @test_list(str);
}

