module test;

extern fn void printf(char*, ...);

fn void test(int x)
{
    int[10] y = { [0..3] = 4, [6..8] = x };
    foreach (int v : y)
    {
        printf("%d\n", v);
    }
}

fn void main()
{
    test(101);
}

