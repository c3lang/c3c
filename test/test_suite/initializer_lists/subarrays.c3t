// #target: x64-darwin

import std::io;
import std::mem;
union Baz
{
    int x;
    double y;
}
struct Bar
{
    int x;
    int y;
}


global Bar[] arrbar = { { 3, 4 }, { 8, 9 }};
global int[] xd = { 1, 2 };
global int* fofeo = &&(int[2]{ 3, 4 });

fn int main()
{
    Bar w = arrbar[1];
    io::printf("%d\n", arrbar[1].x);
    int[] x = { 1, 2, 3 };
    int* y = &&(int[3]{ 123, 234, 567 });
    io::println("Start:");
    io::printf("X len: %d mid element %d\n", (int)(x.len), x[1]);
    io::printf("Y mid element %d\n", y[1]);
    io::printf("Fofeo second element %d\n", fofeo[1]);
    Baz ffe = { .x = 1 };
    int[1] azz = {};
    int[*] a = {};

    //var $foo = { 1, 2, 3 };
    bool xy = ! int[] { 1, 2, 3 };
    if (!xy) io::println("Ok");
    Bar b = {};
    Baz z = {};
    int[] sub = {};
    Bar[] foo = {};
    Baz[3] baz = {};
    return 1;
}

