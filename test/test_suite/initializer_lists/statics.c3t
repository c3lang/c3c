// #target: macos-x64
import std::io;
import libc;
union Baz
{
    int x;
    double y;
}
struct Bar
{
    int x;
    int y;
}


fn void test()
{
    Bar[] b = { { 1, 2 } };
    static Bar[] c = { { 1, 2 } };
    libc::printf("%d %d\n", b[0].y, c[0].y);
    b[0].y += 1;
    c[0].y += 1;

}
fn int main()
{
    test();
    test();
    test();
    return 1;
}

