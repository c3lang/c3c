// #target: x64-darwin

module test;
struct Foo
{
    int x, y;
}

errtype MyErr
{
    FOO
}

extern fn int printf(char *c, ...);

fn void main()
{
    int! z = 2;
    Foo*! w = &&Foo{ z, 0 };
    printf("%d\n", w.x);
    z = MyErr.FOO!;
    w = &&Foo{ z, 0 };
    printf("Not visible: %d\n", w.x);
}

