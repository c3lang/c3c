// #target: macos-x64

fn int hello(int x)
{
    return x + 1;
}

extern fn int printf(char *c, ...);

fn int! tester()
{
    printf("In tester\n");
    return 222;
}

fault Foo
{
    A
}
fn void test1()
{
    int! a = 11;
    if (try int b = a && try int c = tester())
    {
        printf("%d\n", hello(b));
        printf("%d\n", c);
    }
}
fn void main()
{
    test1();
}

fn void test2()
{
    int! a;
    if (try int b = a && hello(b))
    {
        hello(b + 1);
    }
}

