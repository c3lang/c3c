after_assign:                                     ; preds = %after_check, %assign_optional
  %optval = load i64, ptr %has_title.f, align 8
  %not_err5 = icmp eq i64 %optval, 0
  %31 = call i1 @llvm.expect.i1(i1 %not_err5, i1 true)
