// #target: macos-x64

module test;
struct Foo
{
    int x, y;
}

fault MyErr
{
    FOO
}

extern fn int printf(char *c, ...);

fn void main()
{
    int! z = 2;
    Foo*! w = &&Foo{ z, 0 };
    (void)printf("%d\n", w.x);
    z = MyErr.FOO?;
    w = &&Foo{ z, 0 };
    (void)printf("Not visible: %d\n", w.x);
}

