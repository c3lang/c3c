// #target: macos-x64
module test;
import std::io;

fault Test { FOO }

fn void test1()
{
	int! a = 1;
	int! b = a = Test.FOO!;
	if (catch err = a) io::printfn("A err was: %s", err);
	if (catch err = b) io::printfn("B err was: %s", err);
	io::printfn("A was: %s", a);
	io::printfn("B was: %s", b);
}

fn void test2()
{
	int! x = Test.FOO!;
	int! a = 1;
	int! b = a = x;
	if (catch err = a) io::printfn("A err was: %s", err);
	if (catch err = b) io::printfn("B err was: %s", err);
	io::printfn("A was: %s", a);
	io::printfn("B was: %s", b);
}


fn void test3()
{
	int! x = 23;
	int! a = 1;
	int! b = a = x;
	if (catch err = a) io::printfn("A err was: %s", err);
	if (catch err = b) io::printfn("B err was: %s", err);
	io::printfn("A was: %s", a);
	io::printfn("B was: %s", b);
}

fn void main()
{
	test1();
	test2();
	test3();
}

