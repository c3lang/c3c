// #target: macos-x64
module foo;
import std::io;

fault Foo { ABC }

fn void blurb() { io::printn("Blurb");}

macro int! tester()
{
	defer blurb();
	return Foo.ABC?;
}
fn void! test(int x)
{
	io::printfn("test(%d)", x);
	if (x || (tester()!)) io::printn("Ok1");
	io::printn("Test next");
	if (tester()! || x) io::printn("Ok?");
	io::printn("Test ok");
}

fn void! test2(int x)
{
	io::printfn("test2(%d)", x);
	if (x && (tester()!)) io::printn("Ok1");
	io::printn("Test next");
	if ((tester()!) && x) io::printn("Ok?");
	io::printn("Test ok");
}

fn void main()
{
	anyfault a = test(0);
	anyfault b = test(1);
	anyfault c = test2(0);
	anyfault d = test2(1);
}

