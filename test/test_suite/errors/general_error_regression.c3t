module foo;
import std::io;

errtype Foo
{
    X,
    Y,
    Z,
    W,
    W1


}

errtype Foob
{
    X1,
    Y2
}

define Bar = distinct int;

enum MyEnum
{
    A,
    B
}

fn void Foo.hello(Foo *f)
{
    io::println("Hello from Foo");
}

fn void Bar.hello(Bar *b)
{
    io::println("Hello from Bar");
}

fn void MyEnum.hello(MyEnum *myenum)
{
    io::println("Hello from MyEnum");
}
fn void main()
{
    Foo f = Foo.X;
    Foo ef = Foo.Y;
    anyerr x = f;
    ulong z = (ulong)(x);
    io::printf("1: %p\n", z);
    x = ef;
    z = (ulong)(x);
    io::printf("2: %p\n", z);
    x = Foo.W;
    z = (ulong)(x);
    io::printf("21: %p\n", z);
    x = Foo.W1;
    z = (ulong)(x);
    io::printf("22: %p\n", z);
    x = Foob.X1;
    z = (ulong)(x);
    io::printf("3: %p\n", z);
    x = Foob.Y2;
    z = (ulong)(x);
    io::printf("4: %p\n", z);
    Bar b;
    MyEnum a = MyEnum.A;
    f.hello();
    b.hello();
    a.hello();
}

