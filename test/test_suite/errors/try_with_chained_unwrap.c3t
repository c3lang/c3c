  %retparam1 = alloca i8*, align 8
  store i32 0, i32* %val, align 4
  %0 = call i64 @readLine(i8** %retparam1)
  %not_err = icmp eq i64 %0, 0
  br i1 %not_err, label %after.errcheck, label %catch_landing

after.errcheck:                                   ; preds = %entry
