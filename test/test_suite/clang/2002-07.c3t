fn int f0(Quad q, int i) @private {              /* Pass Q by value */
  Quad r @noinit;
  if (i) r.ss = q.ss;
  q.ssp = &r.ss;
  q.w = q.y = q.c = 1;
  return q.ss.y + i + r.y - q.c;
}

fn int f1(Quad *q, int i) {             /* Pass Q by address */
  Quad r @noinit;
  if (i) r = *q;
  q.w = q.y = q.c = 1;
  return q.ss.y+i+r.y-q.c;
}


