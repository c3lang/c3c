// Iterative insert fn
fn void insertIntoListTail(List **l, int data) {
  while (*l) l = &(*l).next;
  *l = (List*)malloc(List.sizeof);
  (*l).data = data;
  (*l).next = null;
}

// Recursive list search fn
fn List *findData(List *l, int data) {
  if (!l) return null;
  if (l.data == data) return l;
  return findData(l.next, data);
}

