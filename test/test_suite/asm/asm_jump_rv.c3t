// #target: elf-riscv32
module test;

def Sum = fn int(int a, int b);

fn int sum(int a, int b)
{
	return a + b;
}

fn void main(String[] args)
{
	int a = 1;
	int b = 2;
	int result = 0;
	Sum sum_ptr = &sum;

	asm
	{
		mv $a0, a;
		mv $a1, b;
		jalr $ra, sum_ptr, 0;
		mv result, $a0;
	}
}


