// #target: macos-x64
module test;

fn void main(String[] args)
{
	int x;
	if (args.len < 10) return;
	asm
	{
		in $eax, 3;
		in $ax, $dx;
		incb $al;
		incw $bx;
		incl $eax;
		incq $rax;
		incl [&x];
		insb;
		insw;
		insl;
		int 0x08;
		int3;
		// int1; Broken in LLVM
		invd;
		invlpg [&x];
		invpcid $rax, [&x];
		invlpga $ecx, $rax;
		iret;
		iretl;
		iretw;
		iretq;
		push 1;
		pushw 2;
		pushw $ax;
		pushw [&x];
		pushq $rax;
		pushq [&x];
	}
}

