struct Map
{
	char[20][] data;
	int width;
}
fn int Map.len(self) @inline @operator(len) => self.width;
fn char[] Map.get(self, int i) @inline @operator([]) => self.data[i][:self.width];

fn void main ()
{
	x.tinit();
	int[3] y = { 1, 2, 3 };
	x.push(y);
	x[0][0] = 4;
	char[20][20] z;
	Map foo = { z[:10], 10 };
	(*&&foo[3])[4] = 123;
}

