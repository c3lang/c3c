// #target: macos-x64
module test;
faultdef ABC;
macro int? tester()
{
	return ABC~;
}
fn void? test2(int )
{
	if (&&(tester()!));
}

fn int main()
{
	fault d = @catch(test2(1));
 	return 0;
}

