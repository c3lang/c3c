module test;

fault Foo
{
	X
}

fn int! test(int i)
{
	return i ?: Foo.X?;
}

fn int! test2(int i)
{
	return i ? Foo.X? : Foo.X?;
}


fn int! test3(int i)
{
	return i ? 2 : Foo.X?;
}


fn int! test4(int i)
{
	int! y = i;
	return (y!) ?: Foo.X?;
}
fn int! test5(int i)
{
	int! y = i;
	return (y!) ? Foo.X? : Foo.X?;
}

fn int! test6(int i)
{
	int! y = i;
	return (y!) ? 2 : Foo.X?;
}

fn int! test7(int i)
{
	int! y = i;
    return (y!) ? Foo.X? : 2;
}

fn void main()
{
	test(1)!!;
	test7(0)!!;
	test6(1)!!;
	int! i = test2(3);
}

