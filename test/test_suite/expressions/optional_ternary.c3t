	return (y?) ? 2 : Foo.X!;
}

fn int! test7(int i)
{
	int! y = i;
    return (y?) ? Foo.X! : 2;
}

fn void main()
{
	test(1)!!;
	test7(0)!!;
	test6(1)!!;
	int! i = test2(3);
