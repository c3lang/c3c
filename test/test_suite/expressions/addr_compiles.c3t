{
    int* w = &XX;
    printf("w = %d (314)\n", *w);
}

struct Foo
{
    int x;
    int y;
}
fn void test3()
{
    Foo h = { 345, 555 };
    int* zx = &h.x;
    int* zy = &h.y;
    int[3] arr = { 333, 444, 999 };
    printf("zx = %d (345) zy = %d (555)\n", *zx, *zy);
    arr[0]--;
    arr[1]--;
    arr[2]--;
    int* d = &arr[2];
    int[]* e = &&arr[1..2];
    printf("d = %d (998) e = %d (443)\n", *d, (*e)[0]);
}

