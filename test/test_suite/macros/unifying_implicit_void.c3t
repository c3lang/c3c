// #target: macos-x64
module oups;
import std::io;

fn void main()
{
	ByteReader r;
	InStream s = &r;
	s.foo()!!;
}

macro InStream.foo(&self)
{
	char! c = self.read_byte();
	if (catch err = c)
	{
		return err?;
	}
	return;
}


