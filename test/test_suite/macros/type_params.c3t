// #target: macos-x64
macro foo($Foo)
{
    $Foo a;
    return a;
}

def Bar = short;
fn void test()
{
    int x = foo(int);
    var $Foo = double;
    double d = foo($Foo);
    double d2 = foo($typeof(d));
    short z = foo(Bar);
}


