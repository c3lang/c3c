}

fn void testi()
{
    int[<4>] y = { 1, 2, 3, 4 };
    int[<4>] z = { 2, 2, 2, -100 };
    int[<4>] w = y + z;
    libc::printf("%d %d %d %d\n", w[0], w[1], w[2], w[3]);
    w = y * z;
    libc::printf("%d %d %d %d\n", w[0], w[1], w[2], w[3]);
    w = y / z;
    libc::printf("%d %d %d %d\n", w[0], w[1], w[2], w[3]);
    w = y - z;
    libc::printf("%d %d %d %d\n", w[0], w[1], w[2], w[3]);
    w = z >> y;
    libc::printf("%d %d %d %d\n", w[0], w[1], w[2], w[3]);
    w = z << y;
    libc::printf("%d %d %d %d\n", w[0], w[1], w[2], w[3]);
    w = z > y;
    libc::printf("%d %d %d %d\n", w[0], w[1], w[2], w[3]);
    uint[<4>] uz = { 2, 6, 2, 1 };
    w = uz > y;
    libc::printf("%d %d %d %d\n", w[0], w[1], w[2], w[3]);
}

fn void main()
{
    testf();
    testi();
    testb();
}

