// #target: x64-darwin
import libc;

fn void testf()
{
    float[<4>] y = { 1, 2, 3, 4 };
    float[<4>] z = { 2, 2, 2, -100 };
    float[<4>] w = y + z;
    libc::printf("%f %f %f %f\n", w[0], w[1], w[2], w[3]);
    w = y * z;
    libc::printf("%f %f %f %f\n", w[0], w[1], w[2], w[3]);
    w = y / z;
    libc::printf("%f %f %f %f\n", w[0], w[1], w[2], w[3]);
    w = y - z;
    libc::printf("%f %f %f %f\n", w[0], w[1], w[2], w[3]);
    int[<4>] ww = y < z;
    libc::printf("%d %d %d %d\n", ww[0], ww[1], ww[2], ww[3]);
}

fn void testb()
