extern fn void printf(char *, ...);

macro bitcast($Target, value)
{
    $typeof(value) temp = value;
    $Target* result = ($Target*)(&temp);
    return *result;
}

fn void main()
{
    float f = 100;
    int i = bitcast(int, f);
    float f2 = bitcast(float, i);
    printf("Bitcast %f to %d to %f", f, i, f2);
}

